<h1 class='center'>Posts for: <%= @feed.category %></h1>
<hr />

<% @posts.each do |p| %>



  <div class="row">
        <div class="col s12">
          <div class="card grey darken-3">
            <div class="card-content white-text">
              <p class="right"><%= p.created_at.strftime("%b %e at %l:%M %p") %></p>
              <br />
              <span class="card-title"><%= p.title %></span>
              <small style="color:yellow;"> - <%= p.category %></small>
              <p><%= p.body %></p>

            </div>
          </div>
        </div>
      </div>
<% end %>
